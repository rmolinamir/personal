{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": ["NITRO_PRESET"],
  "globalPassThroughEnv": ["NITRO_PRESET"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json", "dist/**", ".output/**"]
    },
    "check": {
      "dependsOn": ["^topo", "^build"],
      "outputLogs": "new-only"
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^build"],
      "persistent": true
    },
    "push": {
      "cache": false,
      "interactive": true
    },
    "shadcn": {
      "cache": false,
      "interactive": true
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "topo": {
      "dependsOn": ["^topo"]
    },
    "typecheck": {
      "dependsOn": ["^topo", "^build"]
    }
  },
  "ui": "tui"
}
